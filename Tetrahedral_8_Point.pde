int i = 1;
void setup() {
  size(500, 500);
  background(#668B8B);
  stroke(#FFD1DC);
  strokeWeight(1);
  fill(#668B8B);
  rectMode(CENTER);
}

void draw() {
  background(#668B8B);
  rect(250, 250, 400, 400);

  if (i<300) {
    //1
    line(50, 50, i+100, 100);
    line(50, 50, 400, i+100);
    line(50, 50, (i*-1) + 400, 400);
    line(50, 50, 100, (i*-1) + 400);
    //2
    line(450, 50, i+100, 100);
    line(450, 50, 400, i+100);
    line(450, 50, (i*-1) + 400, 400);
    line(450, 50, 100, (i*-1) + 400);
    //3
    line(50, 450, i+100, 100);
    line(50, 450, 400, i+100);
    line(50, 450, (i*-1) + 400, 400);
    line(50, 450, 100, (i*-1) + 400);
    //4
    line(450, 450, i+100, 100);
    line(450, 450, 400, i+100);
    line(450, 450, (i*-1) + 400, 400);
    line(450, 450, 100, (i*-1) + 400);
    //5
    line(50, 250, i+100, 100);
    line(50, 250, 400, i+100);
    line(50, 250, (i*-1) + 400, 400);
    line(50, 250, 100, (i*-1) + 400);
    //6
    line(250, 450, i+100, 100);
    line(250, 450, 400, i+100);
    line(250, 450, (i*-1) + 400, 400);
    line(250, 450, 100, (i*-1) + 400);
    //7
    line(450, 250, i+100, 100);
    line(450, 250, 400, i+100);
    line(450, 250, (i*-1) + 400, 400);
    line(450, 250, 100, (i*-1) + 400);
    //8
    line(250, 50, i+100, 100);
    line(250, 50, 400, i+100);
    line(250, 50, (i*-1) + 400, 400);
    line(250, 50, 100, (i*-1) + 400);
    rect(i+100, 100, 10, 10);
    rect(400, i+100, 10, 10);
    rect((i*-1) + 400, 400, 10, 10);
    rect(100, (i*-1) + 400, 10, 10);
    i++;
  }
  if (i>=300 && i<600) {
    //1
    line(50, 50, 400, i-200);
    line(50, 50, (i*-1) + 700, 400);
    line(50, 50, 100, (i*-1) + 700);
    line(50, 50, i-200, 100);
    //2
    line(450, 50, 400, i-200);
    line(450, 50, (i*-1) + 700, 400);
    line(450, 50, 100, (i*-1) + 700);
    line(450, 50, i-200, 100);
    //3
    line(50, 450, 400, i-200);
    line(50, 450, (i*-1) + 700, 400);
    line(50, 450, 100, (i*-1) + 700);
    line(50, 450, i-200, 100);
    //4
    line(450, 450, 400, i-200);
    line(450, 450, (i*-1) + 700, 400);
    line(450, 450, 100, (i*-1) + 700);
    line(450, 450, i-200, 100);
    //5
    line(50, 250, 400, i-200);
    line(50, 250, (i*-1) + 700, 400);
    line(50, 250, 100, (i*-1) + 700);
    line(50, 250, i-200, 100);
    //6
    line(250, 450, 400, i-200);
    line(250, 450, (i*-1) + 700, 400);
    line(250, 450, 100, (i*-1) + 700);
    line(250, 450, i-200, 100);
    //7
    line(450, 250, 400, i-200);
    line(450, 250, (i*-1) + 700, 400);
    line(450, 250, 100, (i*-1) + 700);
    line(450, 250, i-200, 100);
    //8
    line(250, 50, 400, i-200);
    line(250, 50, (i*-1) + 700, 400);
    line(250, 50, 100, (i*-1) + 700);
    line(250, 50, i-200, 100);
    rect(400, i-200, 10, 10);
    rect((i*-1) + 700, 400, 10, 10);
    rect(100, (i*-1) + 700, 10, 10);
    rect(i-200, 100, 10, 10);
    i++;
  }
  if (i>=600 && i<900) {
    //1
    line(50, 50, (i*-1) + 1000, 400);
    line(50, 50, 100, (i*-1) +1000);
    line(50, 50, i-500, 100);
    line(50, 50, 400, i-500);
    //2
    line(450, 50, (i*-1) + 1000, 400);
    line(450, 50, 100, (i*-1) +1000);
    line(450, 50, i-500, 100);
    line(450, 50, 400, i-500);
    //3
    line(50, 450, (i*-1) + 1000, 400);
    line(50, 450, 100, (i*-1) +1000);
    line(50, 450, i-500, 100);
    line(50, 450, 400, i-500);
    //4
    line(450, 450, (i*-1) + 1000, 400);
    line(450, 450, 100, (i*-1) +1000);
    line(450, 450, i-500, 100);
    line(450, 450, 400, i-500);
    //5
    line(50, 250, (i*-1) + 1000, 400);
    line(50, 250, 100, (i*-1) +1000);
    line(50, 250, i-500, 100);
    line(50, 250, 400, i-500);
    //6
    line(250, 450, (i*-1) + 1000, 400);
    line(250, 450, 100, (i*-1) +1000);
    line(250, 450, i-500, 100);
    line(250, 450, 400, i-500);
    //7
    line(450, 250, (i*-1) + 1000, 400);
    line(450, 250, 100, (i*-1) +1000);
    line(450, 250, i-500, 100);
    line(450, 250, 400, i-500);
    //8
    line(250, 50, (i*-1) + 1000, 400);
    line(250, 50, 100, (i*-1) +1000);
    line(250, 50, i-500, 100);
    line(250, 50, 400, i-500);
    rect((i*-1) + 1000, 400, 10, 10);
    rect(100, (i*-1) +1000, 10, 10);
    rect(i-500, 100, 10, 10);
    rect(400, i-500, 10, 10);
    i++;
  }
  if (i>=900 && i< 1200) {
    //1
    line(50, 50, 100, (i*-1) + 1300);
    line(50, 50, i-800, 100);
    line(50, 50, 400, i-800);
    line(50, 50, (i*-1) + 1300, 400);
    //2
    line(450, 50, 100, (i*-1) + 1300);
    line(450, 50, i-800, 100);
    line(450, 50, 400, i-800);
    line(450, 50, (i*-1) + 1300, 400);
    //3
    line(50, 450, 100, (i*-1) + 1300);
    line(50, 450, i-800, 100);
    line(50, 450, 400, i-800);
    line(50, 450, (i*-1) + 1300, 400);
    //4
    line(450, 450, 100, (i*-1) + 1300);
    line(450, 450, i-800, 100);
    line(450, 450, 400, i-800);
    line(450, 450, (i*-1) + 1300, 400);
    //5
    line(50, 250, 100, (i*-1) + 1300);
    line(50, 250, i-800, 100);
    line(50, 250, 400, i-800);
    line(50, 250, (i*-1) + 1300, 400);
    //6
    line(250, 450, 100, (i*-1) + 1300);
    line(250, 450, i-800, 100);
    line(250, 450, 400, i-800);
    line(250, 450, (i*-1) + 1300, 400);
    //7
    line(450, 250, 100, (i*-1) + 1300);
    line(450, 250, i-800, 100);
    line(450, 250, 400, i-800);
    line(450, 250, (i*-1) + 1300, 400);
    //8
    line(250, 50, 100, (i*-1) + 1300);
    line(250, 50, i-800, 100);
    line(250, 50, 400, i-800);
    line(250, 50, (i*-1) + 1300, 400);
    rect(100, (i*-1)+1300, 10, 10);
    rect(i-800, 100, 10, 10);
    rect(400, i-800, 10, 10);
    rect((i*-1)+1300, 400, 10, 10);
    i++;
  }
  if (i>= 1200) {
    i = 1;
  }
}

